<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maple.keepalive">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--excludeFromRecents 任务栈不可见-->
        <activity android:name=".piexlHelp.KeepActivity"
            android:theme="@style/tran"
            android:excludeFromRecents="true"
            android:launchMode="singleTask"
            android:taskAffinity="com.maple.keepalive"/>

        <service android:name=".ForegroundService" />
        <service android:name=".ForegroundService$InnerService" />
        <service android:name=".doubleThread.LocalService" />
        <service
            android:name=".doubleThread.RemoteService"
            android:process=":remote" />
    </application>

</manifest>